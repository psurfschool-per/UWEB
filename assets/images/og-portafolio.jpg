<!-- Open Graph image for portfolio page -->
<svg width="1200" height="630" viewBox="0 0 1200 630" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="portfolioGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ff9a9e;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#fecfef;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="1200" height="630" fill="url(#portfolioGradient)"/>
  
  <!-- Logo -->
  <rect x="80" y="80" width="100" height="100" rx="15" fill="white" opacity="0.9"/>
  <text x="130" y="140" text-anchor="middle" fill="#ff9a9e" font-family="Arial, sans-serif" font-size="28" font-weight="bold">UWEB</text>
  
  <!-- Title -->
  <text x="220" y="140" fill="white" font-family="Arial, sans-serif" font-size="42" font-weight="bold">Nuestro Portafolio</text>
  <text x="220" y="180" fill="white" font-family="Arial, sans-serif" font-size="24" opacity="0.9">Casos de éxito y proyectos destacados</text>
  
  <!-- Project Cards -->
  <g transform="translate(220, 220)">
    <!-- Card 1 -->
    <rect x="0" y="0" width="150" height="100" rx="10" fill="white" opacity="0.9"/>
    <text x="75" y="30" text-anchor="middle" fill="#ff9a9e" font-family="Arial, sans-serif" font-size="14" font-weight="bold">E-commerce</text>
    <text x="75" y="50" text-anchor="middle" fill="#666" font-family="Arial, sans-serif" font-size="12">Moda Peruana</text>
    <text x="75" y="70" text-anchor="middle" fill="#999" font-family="Arial, sans-serif" font-size="10">Tienda Online</text>
    
    <!-- Card 2 -->
    <rect x="170" y="0" width="150" height="100" rx="10" fill="white" opacity="0.9"/>
    <text x="245" y="30" text-anchor="middle" fill="#ff9a9e" font-family="Arial, sans-serif" font-size="14" font-weight="bold">Restaurante</text>
    <text x="245" y="50" text-anchor="middle" fill="#666" font-family="Arial, sans-serif" font-size="12">Sabores del Norte</text>
    <text x="245" y="70" text-anchor="middle" fill="#999" font-family="Arial, sans-serif" font-size="10">Web Corporativa</text>
    
    <!-- Card 3 -->
    <rect x="340" y="0" width="150" height="100" rx="10" fill="white" opacity="0.9"/>
    <text x="415" y="30" text-anchor="middle" fill="#ff9a9e" font-family="Arial, sans-serif" font-size="14" font-weight="bold">Clínica</text>
    <text x="415" y="50" text-anchor="middle" fill="#666" font-family="Arial, sans-serif" font-size="12">Salud Integral</text>
    <text x="415" y="70" text-anchor="middle" fill="#999" font-family="Arial, sans-serif" font-size="10">Sistema Citas</text>
  </g>
  
  <!-- Stats -->
  <g transform="translate(220, 380)">
    <text x="0" y="0" fill="white" font-family="Arial, sans-serif" font-size="32" font-weight="bold">100+</text>
    <text x="0" y="25" fill="white" font-family="Arial, sans-serif" font-size="16" opacity="0.8">Proyectos</text>
    
    <text x="150" y="0" fill="white" font-family="Arial, sans-serif" font-size="32" font-weight="bold">50+</text>
    <text x="150" y="25" fill="white" font-family="Arial, sans-serif" font-size="16" opacity="0.8">Clientes</text>
    
    <text x="280" y="0" fill="white" font-family="Arial, sans-serif" font-size="32" font-weight="bold">5+</text>
    <text x="280" y="25" fill="white" font-family="Arial, sans-serif" font-size="16" opacity="0.8">Años</text>
  </g>
  
  <!-- CTA -->
  <rect x="220" y="480" width="250" height="60" rx="30" fill="white"/>
  <text x="345" y="520" text-anchor="middle" fill="#ff9a9e" font-family="Arial, sans-serif" font-size="20" font-weight="bold">Ver Proyectos</text>
  
  <!-- Decorative -->
  <circle cx="900" cy="200" r="40" fill="white" opacity="0.1"/>
  <circle cx="1000" cy="350" r="60" fill="white" opacity="0.1"/>
</svg>